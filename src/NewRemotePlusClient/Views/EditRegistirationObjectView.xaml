<Window x:Class="NewRemotePlusClient.Views.EditRegistirationObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewRemotePlusClient.Views"
        mc:Ignorable="d"
        Title="EditRegistirationObject" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25" />
            <RowDefinition Height="*" />
            <RowDefinition Height="30" />
            <RowDefinition Height="25" />
        </Grid.RowDefinitions>
        <PropertyGrid Grid.Row="1" SelectedO="{Binding RegisterObject}" />
        <StackPanel Grid.Row="2" Orientation="Horizontal">
            <Button Content="OK" Width="100"/>
            <Button Content="Cancel" Width="100" />
        </StackPanel>
    </Grid>
</Window>
